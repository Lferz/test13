local imageUrl = "https://sun9-15.userapi.com/impf/c840725/v840725214/755be/O_QA9st-YKQ.jpg?size=600x489&quality=96&proxy=1&sign=3294d799773798da4ba84e63cd785aac&type=album"

-- Создание объекта ImageLabel
local imageLabel = Instance.new("ImageLabel")
imageLabel.Image = imageUrl
imageLabel.Size = UDim2.new(0, 200, 0, 200)

-- Добавление ImageLabel на экран игры
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local screenGui = Instance.new("ScreenGui")
imageLabel.Parent = screenGui
screenGui.Parent = playerGui
